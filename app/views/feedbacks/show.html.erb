<!-- Simple full-width example-->

<div class="page-header">
  <h1>Feedback #<%= @feedback.id %></h1>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Writer</dt>
      <dd><%= @feedback.writer_id %></dd>

      <dt>Receiver</dt>
      <dd><%= @feedback.receiver_id %></dd>

      <dt>Goal</dt>
      <dd><%= @feedback.goal_id %></dd>

      <dt>Interaction</dt>
      <dd><%= @feedback.interaction_id %></dd>

      <dt>Request</dt>
      <dd><%= @feedback.request_id %></dd>

      <dt>Qualitative fb</dt>
      <dd><%= @feedback.qualitative_fb %></dd>

      <dt>Quantitative fb</dt>
      <dd><%= @feedback.quantitative_fb %></dd>

      <dt></dt>
      <dd>
        <div class="btn-group">
          <a href="/feedbacks" class="btn btn-primary">
            <i class="fa fa-chevron-left"></i>
          </a>

          <a href="/feedbacks/<%= @feedback.id %>/edit" class="btn btn-warning">
            <i class="fa fa-edit"></i>
          </a>

          <a href="/delete_feedback/<%= @feedback.id %>" class="btn btn-danger">
            <i class="fa fa-trash-o"></i>
          </a>
        </div>
      </dd>
    </dl>

  </div>
</div>

<!-- End simple full-width example-->

<!-- Responsive panel example-->

<div class="row">
  <div class="col-sm-offset-1 col-sm-10 col-md-offset-2 col-md-8">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">Feedback #<%= @feedback.id %></h3>
      </div>
      <div class="panel-body">
        <div class="row">
          <div class="col-md-12">
            <dl class="dl-horizontal">
              <dt>Writer</dt>
              <dd><%= @feedback.writer_id %></dd>

              <dt>Receiver</dt>
              <dd><%= @feedback.receiver_id %></dd>

              <dt>Goal</dt>
              <dd><%= @feedback.goal_id %></dd>

              <dt>Interaction</dt>
              <dd><%= @feedback.interaction_id %></dd>

              <dt>Request</dt>
              <dd><%= @feedback.request_id %></dd>

              <dt>Qualitative fb</dt>
              <dd><%= @feedback.qualitative_fb %></dd>

              <dt>Quantitative fb</dt>
              <dd><%= @feedback.quantitative_fb %></dd>

            </dl>
            <div class="btn-group btn-group-justified">
              <a href="/feedbacks" class="btn btn-primary">
                <i class="fa fa-chevron-left"></i>
              </a>

              <a href="/feedbacks/<%= @feedback.id %>/edit" class="btn btn-warning">
                <i class="fa fa-edit"></i>
              </a>

              <a href="/delete_feedback/<%= @feedback.id %>" class="btn btn-danger">
                <i class="fa fa-trash-o"></i>
              </a>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<!-- End responsive panel example -->
